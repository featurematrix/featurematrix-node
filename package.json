{
    "name": "featurematrix-node",
    "version": "0.0.1",
    "description": "FeatureMatrix Node SDK",
    "main": "index.js",
    "scripts": {
        "start": "node dist/index.js",
        "build": "rm -rf dist && tsc",
        "serve": "TS_NODE_FILES=true nodemon --watch 'src' -r ts-node/register src/index.ts",
        "debug": "TS_NODE_FILES=true nodemon --inspect --watch 'src' -r ts-node/register src/index.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@gitlab.com/featurematrix/featurematrix-node.git"
    },
    "keywords": [
        "featurematrix",
        "feature",
        "flag",
        "toggle"
    ],
    "author": "Nairi Narinyan",
    "license": "UNLICENSED",
    "bugs": {
        "url": "https://gitlab.com/featurematrix/featurematrix-node/issues"
    },
    "homepage": "https://gitlab.com/featurematrix/featurematrix-node#readme",
    "dependencies": {
        "ws": "^7.0.0"
    },
    "devDependencies": {
        "@types/node": "^12.0.2",
        "@types/ws": "^6.0.1"
    }
}
